{
  "name": "travis-size-report",
  "version": "1.1.0",
  "description": "Compare files from one build to another",
  "main": "build/index.js",
  "bin": {
    "sizereport": "build/bin.js"
  },
  "scripts": {
    "clean": "rm -rf build && rm -rf assets/build",
    "build": "npm run clean && rollup -c",
    "watch": "npm run clean && rollup -c --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChromeLabs/travis-size-report.git"
  },
  "author": "Jake Archibald",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChromeLabs/travis-size-report/issues"
  },
  "homepage": "https://github.com/GoogleChromeLabs/travis-size-report#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "escape-string-regexp": "^2.0.0",
    "glob": "^7.1.3",
    "gzip-size": "^5.1.0",
    "minimist": "^1.2.0",
    "node-fetch": "^2.3.0",
    "pretty-bytes": "^5.1.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/minimist": "^1.2.0",
    "@types/node-fetch": "^2.3.2",
    "@types/pretty-bytes": "^5.1.0",
    "husky": "^1.3.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-typescript2": "^0.21.2",
    "tslib": "^1.10.0",
    "typescript": "^3.4.4"
  },
  "files": [
    "build",
    "README.md"
  ]
}
